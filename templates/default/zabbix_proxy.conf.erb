# Generated by chef.

############ GENERAL PARAMETERS #################

<% if node['zabbix']['proxy']['log_file'] -%>
LogFile=<%= node['zabbix']['proxy']['log_file'] %>
<% end -%>

<% unless node["zabbix"]["proxy"]["database"]["install_method"] == 'sqlite' -%>
DBHost=<%= @dbhost %>
DBName=<%= @dbname %>
DBUser=<%= @dbuser %>
DBPassword=<%= @dbpassword %>
DBPort=<%= @dbport %>
<% end -%>

<% if node['zabbix']['server']['java_gateway_enable'] -%>
JavaGateway=<%= @java_gateway %>
JavaGatewayPort=<%= @java_gateway_port %>
StartJavaPollers=<%= @java_pollers %>
<% end -%>

StartPollers = <%= node['zabbix']['server']['start_pollers'] %>
ProxyOfflineBuffer = <% node['zabbix']['proxy']['proxy_offline_buffer'] %>
CacheSize = <% node['zabbix']['proxy']['cache_size'] %>
DataSenderFrequency = <% node['zabbix']['proxy']['data_sender_frequency'] %>
ConfigFrequency = <% node['zabbix']['proxy']['config_frequency'] %>
HeartbeatFrequency = <% node['zabbix']['proxy']['heartbeat_frequency'] %>
